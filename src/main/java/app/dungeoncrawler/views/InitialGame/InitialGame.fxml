<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<?import javafx.scene.text.Font?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            styleClass="hBoxMainContainer"
            stylesheets="@InitialGame.css"
            fx:controller="app.dungeoncrawler.views.InitialGame.InitialGameController"
>
    <Text styleClass="scoreBox"
          textAlignment="LEFT"
          id="score"
    >
        $
    </Text>
    <Text fx:id="money" fill="black" layoutX="80" layoutY="55" text="90">
        <font>
            <Font name="verdana" size="15"/>
        </font>
    </Text>
</AnchorPane>
