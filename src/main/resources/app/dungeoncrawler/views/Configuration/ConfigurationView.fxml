<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane gridLinesVisible="true" stylesheets="@ConfigurationView.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="app.dungeoncrawler.views.Configuration.ConfigurationViewController">
    <children>
        <Label styleClass="helloworldText" text="CONFIGURARION" GridPane.rowIndex="1" />
      <ImageView fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true" GridPane.rowIndex="2" />
      <Group GridPane.columnIndex="1" GridPane.rowIndex="2" />
        <TextField fx:id="textfield" GridPane.rowIndex="3" />
        <Button fx:id="buttonNavigate" prefHeight="29.0" prefWidth="67.0" text="start" GridPane.columnIndex="4" GridPane.rowIndex="5">
         <font>
            <Font size="20.0" />
         </font></Button>
        <Label text="${textfield.text}" GridPane.columnSpan="2" GridPane.rowIndex="2" />

    </children>
   <columnConstraints>
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
   </rowConstraints>
</GridPane>
